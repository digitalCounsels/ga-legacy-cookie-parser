{
    "title": "GA Legacy Cookie Parser",
    "name": "ga-legacy-cookie-parser-js",
    "version": "0.0.1",
    "author": "Matthias Isler <mi@matthias-isler.ch>",
    "description": "A workaround for accessing GA data in Universal Analytics environments.",
    "keywords": [
        "analytics",
        "google-analytics",
        "universal-analytics",
        "cookie",
        "cookie-parkser",
        "utma",
        "utmb",
        "utmc",
        "utmz"
    ],
    "homepage": "https://github.com/spiritix/ga-legacy-cookie-parser",
    "main": "src/ga-legacy-cookie-parser.js",
    "scripts": {
        "build": "uglifyjs src/ga-legacy-cookie-parser.js -o dist/ga-legacy-cookie-parser.min.js --comments '/GAParser\\.js/' && uglifyjs src/ga-legacy-cookie-parser.js -o dist/ga-legacy-cookie-parser.pack.js --comments '/GAParser\\.js/' --compress --mangle",
        "test": "jshint src/ga-legacy-cookie-parser.js && mocha -R nyan test/test.js",
        "verup": "node ./node_modules/verup",
        "version": "node ./node_modules/verup 0"
    },
    "verup": {
        "files": [
            "bower.json",
            "package.js",
            "src/ga-legacy-cookie-parser.js"
        ],
        "regs": [
            "^((?:\\$|(\\s*\\*\\s*@)|(\\s*(?:var|,)?\\s+))(?:LIBVERSION|version)[\\s\\:='\"]+)([0-9]+(?:\\.[0-9]+){2,2})",
            "^(\\s?\\*.*v)([0-9]+(?:\\.[0-9]+){2,2})"
        ]
    },
    "devDependencies": {
        "jshint": "~1.1.0",
        "mocha": "~1.8.0",
        "requirejs": "^2.3.2",
        "uglify-js": "~2.7.5",
        "verup": "^1.3.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spiritix/ga-legacy-cookie-parser.git"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "dist": "dist",
        "src": "src",
        "test": "test"
    },
    "bugs": "https://github.com/spiritix/ga-legacy-cookie-parser/issues",
    "download": "https://raw.github.com/spiritix/ga-legacy-cookie-parser/master/dist/ga-legacy-cookie-parser.min.js"
}